#include<stdio.h>

const float THRESHOLD = 0.000001;

void searchElementInTheList (float *a, float ele, int n) {

    int i;

    for (i = 0; i < n; i++) {

        if (((ele - a[i]) <= THRESHOLD && (ele - a[i]) >= -THRESHOLD) || ((a[i] - ele) <= THRESHOLD && (a[i] - ele) >= -THRESHOLD))
        {
            printf("YES\n");
            return;
        }
    }
    printf("NO\n");

}

int main () {

    int n;
    printf("Enter number of elements in the array : (max. 100) \n");
    scanf("%d", &n);
    if (n > 100) {
        printf("ERROR : Number of array should be less than 100\n");
        return 1;
    }

    float list[100];
    printf("ENTER THE ELEMENTS OF THE LIST (PRECISION UPTO .000001) :\n");
    int i;
    int temp;

    for (i = 0; i < n; i++) {

        scanf("%f", &list[i]);
        if (i == 0) {
            temp = list[i];
        }
        else {
            if (temp - list[i] > THRESHOLD) {
                printf("ERROR : UNSORTED DATA \n");
                return 1;
            }
            temp = list[i];
        }
    }

    float searchElement;
    printf("ENTER THE SEARCH ELEMENT : \n");
    scanf("%f", &searchElement);

    searchElementInTheList (list, searchElement, n);
    return 0;

}
